<UserControl
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:ui="using:EstragoniaTemplate.UI"
	xmlns:vm="using:EstragoniaTemplate.UI.ViewModels"
	xmlns:controls="using:EstragoniaTemplate.UI.Controls"
	x:Class="EstragoniaTemplate.UI.Controls.FocusCapture">

	<Border Padding="200" Focusable="False" XYFocus.NavigationModes="Disabled">
		<StackPanel>
			<ContentPresenter Content="{Binding $parent[2].Content}" />			
		</StackPanel>
	</Border>
</UserControl>